<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.Woocer.Headline1" parent="TextAppearance.MaterialComponents.Headline1">
        <item name="fontFamily">@font/highpro</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">28sp</item>
    </style>


    <style name="TextAppearance.Woocer.Subtitle1" parent="TextAppearance.MaterialComponents.Subtitle1">
        <item name="fontFamily">@font/highpro</item>
        <item name="android:textColor">@color/gray_100</item>
        <item name="lineHeight">22sp</item>
    </style>


    <style name="TextAppearance.Woocer.Body1" parent="TextAppearance.MaterialComponents.Body1">
        <item name="fontFamily">@font/highpro</item>
        <item name="android:textColor">@color/black</item>
        <item name="lineHeight">18sp</item>
    </style>

    <style name="TextAppearance.Woocer.Button" parent="TextAppearance.MaterialComponents.Button">
        <item name="fontFamily">@font/highpro</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.WoocerTab" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textSize">28sp</item>
        <item name="android:fontFamily">@font/highpro</item>
        <item name="android:textColor">@color/black</item>
    </style>

</resources>
